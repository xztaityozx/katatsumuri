-enc=utf8
-ole=unix
-i=2
-l=120
--check-syntax

--closing-token-indentation=1
--nooutdent-long-quotes
--nooutdent-long-lines

--nospace-for-semicolon
--nospace-terminal-semicolon
--add-semicolons

--opening-brace-always-on-right

# イコールを揃えない
-vxl='='
# ()の前後にスペースをいれない
-pt=2
# []の前後にスペースを入れない
-sbt=2
# プロトタイプの前にスペースを入れない
-spp=0
